{
  "name": "onlinemlm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:seed:local": "cross-env DB_LOCAL_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) tsx ./db/seed.ts",
    "db:setup:prod": "node -r esbuild-register db/setup.ts",
    "db:migrate:local": "wrangler d1 migrations apply db --local",
    "db:migrate:preview": "wrangler d1 migrations apply --env preview my-preview-database-name --remote",
    "db:migrate:prod": "wrangler d1 migrations apply db --remote",
    "db:studio:local": "cross-env DB_LOCAL_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:preview": "cross-env NODE_ENV='preview' drizzle-kit studio",
    "db:studio:prod": "drizzle-kit studio",
    "pages:build": "pnpm next-on-pages",
    "dev": "next dev",
    "build": "next build",
    "pages:dev": "pnpm pages:build && pnpm wrangler pages dev",
    "pages:deploy:preview": "pnpm pages:build && pnpm wrangler pages deploy --branch preview",
    "pages:deploy": "pnpm pages:build && pnpm wrangler pages deploy",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.675.0",
    "@aws-sdk/s3-request-presigner": "^3.675.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@mdxeditor/editor": "^3.17.2",
    "@next/mdx": "^15.0.2",
    "@notionhq/client": "^2.2.15",
    "bcrypt-edge": "^0.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "drizzle-orm": "^0.32.0",
    "drizzle-zod": "^0.5.1",
    "framer-motion": "^11.11.9",
    "lru-cache": "^11.0.1",
    "lucia": "^3.2.1",
    "nanoid": "^5.0.7",
    "next": "14.2.4",
    "next-intl": "^3.24.0",
    "next-mdx-remote": "^5.0.0",
    "next-nprogress-bar": "^2.3.14",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.53.0",
    "react-modern-drawer": "^1.4.0",
    "swiper": "^11.1.14",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@cloudflare/workers-types": "^4.20240712.0",
    "@haydenull/fabric": "^0.2.6",
    "@libsql/client": "^0.14.0",
    "@next/bundle-analyzer": "^15.0.2",
    "@next/env": "^14.2.5",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/react-query-devtools": "^5.50.1",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.23.0",
    "esbuild-register": "^3.6.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.16.2",
    "typescript": "^5",
    "unplugin-turbo-console": "^1.9.11",
    "wrangler": "^3.81.0"
  }
}
