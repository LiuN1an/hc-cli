{
    "name": "electron-react",
    "version": "1.0.0",
    "description": "test electron",
    "homepage": ".",
    "main": "dist/main/main.prod.js",
    "build": {
        "productName": "Electron test",
        "appId": "electron.test.app",
        "asar": false,
        "files": [
            "dist/",
            "node_modules/",
            "resources/",
            "native/",
            "package.json",
            "src/main/preload.js"
        ],
        "directories": {
            "buildResources": "resources",
            "output": "release"
        },
        "win": {
            "icon": "resources/1.ico",
            "target": [
                "nsis",
                "msi"
            ]
        }
    },
    "scripts": {
        "build:main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
        "build:renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
        "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
        "start:main": "electron ./dist/main/main.prod.js",
        "dev:main": "cross-env PORT=3000 NODE_ENV=development webpack --config webpack.main.dev.config.js && electron ./dist/main/main.dev.js",
        "dev:renderer": "cross-env PORT=3000 NODE_ENV=development webpack serve --config webpack.renderer.dev.config.js",
        "dev": "cross-env PORT=8899 & npm run dev:renderer",
        "pck:win": "npm run build && electron-builder build --win --x64"
    },
    "author": "LiuN1an",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.14.3",
        "@babel/core": "^7.14.3",
        "@babel/plugin-proposal-decorators": "^7.17.2",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
        "@types/electron": "^1.6.10",
        "@types/node": "^17.0.21",
        "@types/react": "^17.0.40",
        "@types/react-dom": "^17.0.13",
        "@types/webpack": "^5.28.0",
        "babel-loader": "^8.2.3",
        "buffer": "^6.0.3",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^7.0.0",
        "core-js": "^3.13.1",
        "cross-env": "^7.0.3",
        "crypto-browserify": "^3.12.0",
        "css-loader": "^5.2.7",
        "electron": "^13.1.1",
        "electron-builder": "^22.14.13",
        "electron-rebuild": "^2.3.5",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "https-browserify": "^1.0.0",
        "less": "^4.1.1",
        "less-loader": "^9.1.0",
        "os-browserify": "^0.3.0",
        "path-browserify": "^1.0.1",
        "process": "^0.11.10",
        "react-refresh": "^0.11.0",
        "regenerator-runtime": "^0.13.7",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "style-loader": "^2.0.0",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "typescript": "^4.6.2",
        "util": "^0.12.4",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^3.11.3",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0",
        "autoprefixer": "^10.4.7",
        "tailwindcss": "^3.0.24",
        "tailwindcss-dark-mode": "^1.1.7",
        "postcss": "^8.4.13",
        "postcss-loader": "^6.2.1",
        "postcss-selector-parser": "^6.0.10"
    },
    "dependencies": {
        "@electron/remote": "^2.0.8",
        "@types/ffi-napi": "^4.0.5",
        "assert": "^2.0.0",
        "classnames": "^2.3.1",
        "ffi-napi": "^4.0.3",
        "mobx": "^6.5.0",
        "mobx-react-lite": "^3.3.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.2.0",
        "ref-napi": "^3.0.3"
    }
}